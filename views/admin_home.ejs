<html>

<head>
    <title>this is da page</title>
</head>

<body>
    <table>
        <h1>These are the Studies i have it</h1>
        <tr>
            <th>Study Title</th>
            <th>Study Info</th>
            <th>Study Link</th>
        </tr>
        <% for (const row of query_result) { %>
            <% if (row.is_active == 1) { %>
                <tr>
                    <td><%= row.title %></td>
                    <td><%= row.info %></td>
                    <td><a href="/adminhome/study/<%= row.study_id %>">Go to study</a></td>
                </tr>
            <% } %>
        <% } %>
    </table>
</body>

</html>